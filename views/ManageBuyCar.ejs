                                            style="font-size: 15px;" data-toggle="modal" data-target="#invoiceModal"
                                            onclick="feedInvoice2('<%= cus[i].customer_first_name %>',
                                            '<%= cus[i].customer_last_name %>','<%= cus[i].address.address_id %>',
