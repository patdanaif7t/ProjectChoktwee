                                    <th>ทะเบียนรถ</th>
                                    <th>ยี่ห้อ</th>
                                    <th>รุ่น</th>
                                    <th>ราคาขาย</th>
                                    <th>สถานะ</th>
                                    <th style="width: 122px">ออกใบเสร็จ</th>
                                    <th style="width: 122px">ออกใบกำกับภาษี</th>
                                </tr>
                            </thead>
                            <tbody align="center">
                                <% for(var i=0; i < cars.length; i++) { %>
                                <tr>
                                    <td>
                                        <%= cars[i].ID_MST_car %>
                                    </td>
                                    <td>
                                        <%= cars[i].car_license %>
                                    </td>
                                    <td>
                                        <%= cars[i].car_company %>
                                    </td>
                                    <td>
                                        <%= cars[i].car_model %>
                                    </td>
                                    <td>
                                        <%= cars[i].car_price_sell %>
                                    </td>
                                    <td>
                                        <%= cars[i].car_status %>
                                    </td>
                                    <td align="center">
                                        <button type="button" class="btn btn-outline-info btn-block"
                                            style="font-size: 15px;" data-toggle="modal"
                                            data-target="#exampleModalCenter"
                                            onclick="feedInvoice('<%= cus[i].customer_first_name %>',
